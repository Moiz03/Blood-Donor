<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#121212</item>
        <item name="colorAccent">#FF0000</item>
        <item name="titleTextColor">#FF0000</item>
        <item name="android:actionMenuTextColor">#FF0000</item>
    </style>
    
    <style name="mytoast">
        <item name="stColorBackground">#FF0000</item>
        <item name="stTextColor">#FFFFFF</item>
        <item name="stStrokeColor">#FFFFFF</item>
        <item name="stStrokeWidth">3dp</item>
        <item name="stIconStart">@drawable/call_me</item>
        <item name="stLength">LONG</item>
    </style>

    <style name="fab">
        <item name="stColorBackground">#FFFFFF</item>
    </style>


    <style name="builder" >
        <item name="android:textColor">#FF0000</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">#121212</item>
    </style>



</resources>
